<h1> Instructor List</h1>

<table id="datatable" class="display" data-turbolinks="false">
    <thead>
        <tr class="table-header">
            <th>First Name</th>
            <th>Last Name</th>
            <th>Birthdate</th>
            <th>Type</th>
            <th class='center-col'>Email</th>
            <% if has_role?("Admin") %>
            <th>Salary</th>
            <% end %>
            <th>Education</th>
            <th class='center-col'>Options</th>
            <% if has_role?("Admin") %>
            <th></th>
            <th></th>
            <% end %>
        </tr>
    </thead>
    <tbody>
        <%= render "instructor_list" %>
    </tbody>
</table>